{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/public-client.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "QWEN_API_KEY": "sk-bddda4e9e2ef4aa5acdb773207ac4036",
    "DEFAULT_PROVIDER": "qwen",
    "ENABLE_API_KEY_INPUT": "false",
    "NODE_ENV": "production"
  }
}